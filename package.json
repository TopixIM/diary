  "dependencies": {
    "copy-to-clipboard": "^3.3.1",
    "luxon": "^1.25.0",
    "md5": "^2.3.0",
    "randomcolor": "^0.6.2",
    "shortid": "^2.2.16",
    "ws": "^7.4.1"
  },
  "devDependencies": {
    "feather-icons": "^4.28.0",
    "http-server": "^0.12.3",
    "node-notifier": "^9.0.0",
    "shadow-cljs": "^2.11.10",
    "source-map-support": "^0.5.19"
  }
